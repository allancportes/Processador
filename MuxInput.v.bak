module MuxInput (input1, input2, out, select, switch, inputControl); //Mux de saida da memÃ³ria de dados

  input select;
  input [31:0] input1, input2;
  output reg [31:0] out;
  input [15:0] switch;
  input inputControl;

 always @ (*) begin
	
	if(select) begin
		out = input2;
	end
	else begin
		out = input1;
	end
		


endmodule
